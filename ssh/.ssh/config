# Added by OrbStack: 'orb' SSH host for Linux machines
# This only works if it's at the top of ssh_config (before any Host blocks).
# Comment this line if you don't want it to be added again.
Include ~/.orbstack/ssh/config

Host distiller
  HostName distiller.tail5f4f1f.ts.net
  User distiller
  IdentityFile ~/.ssh/distiller
  AddKeysToAgent yes

Host dev-server
  HostName dev-server.tail5f4f1f.ts.net
  User admin
  IdentityFile ~/.ssh/distiller-dev.pem
  AddKeysToAgent yes

Host bitbucket.org
  AddKeysToAgent yes
  IdentityFile ~/.ssh/bitbucket_work
